#include<stdio.h>
#include<conio.h>
int swap(int* a ,int* b){
    int temp = a*;
    *a = *b;
    *b = temp;
}
int partition(int first, int last, int arr[]){
        int pivot = arr[last];
        int i = first-1;
        for(int j =0; j<=last-1; j++){
             if(a[j]<pivot){
                swap(&a[j],&a[i]);
             }
        }

}
